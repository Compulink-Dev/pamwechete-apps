{
  "expo": {
    "name": "Pamwechete",
    "slug": "pamwechete-mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/logo-white.png",
      "resizeMode": "contain",
      "backgroundColor": "#003C5C",
      "imageStyle": {
        "width": 200,
        "height": 200,
        "margin": 40
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pamwechete.mobile",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#003C5C"
      },
      "package": "com.pamwechete.mobile"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-font"
    ],
    "scheme": "pamwechete",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "cb784b21-e547-499e-8bd6-bb07c358203e"
      }
    }
  }
}
